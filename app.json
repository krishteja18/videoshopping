{
  "expo": {
    "name": "SwipeKart",
    "slug": "SwipeKart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "swipekart",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      "@react-native-google-signin/google-signin",
      "expo-router",
      "expo-secure-store"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.swipekart.app"
    },
    "android": {
      "package": "com.swipekart.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "19256bf8-f651-4d1b-bc50-834c31eeb3de"
      }
    }
  }
}
