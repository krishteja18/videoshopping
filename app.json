{
  "expo": {
    "name": "SwipeKart",
    "slug": "SwipeKart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "swipekart",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      "@react-native-google-signin/google-signin",
      "expo-router"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.swipekart.app"
    },
    "android": {
      "package": "com.swipekart.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
